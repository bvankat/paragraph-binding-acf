{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "acf-paragraph-block/dynamic",
	"title": "ACF Field Paragraph",
	"category": "text",
	"icon": "admin-comments",
	"description": "A paragraph block variation that displays the value of an ACF field.",
	"keywords": [ "acf", "paragraph", "custom field" ],
	"supports": {
		"color": {
			"text": true,
			"background": true
		},
		"spacing": {
			"margin": true,
			"padding": true
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"textAlign": true
		},
		"border": true,
		"html": false
	},
	"attributes": {
		"acfFieldSlug": {
			"type": "string",
			"default": ""
		}
	},
	"editorScript": "acf-paragraph-block-js",
	"style": "acf-paragraph-block-style",
	"render": "file:./render.php"
}
